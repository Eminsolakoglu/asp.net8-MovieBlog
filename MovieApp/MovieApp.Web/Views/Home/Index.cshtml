
@model Movie
@{
    Layout = "_Layout2";
}
<div class="film">
    <h1> Film bilgileri</h1>
    
    <p>@Model.Title </p>
    <p>@Model.Description </p>
    <p>@Model.Director </p>
     <ul>
        @foreach (var actor in Model.Actors)
        {
           <li>@actor</li>
        }
     </ul>
</div>
